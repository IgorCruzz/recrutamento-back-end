  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "prestart": "rimraf dist && ts-node -p tsconfig.build.json",
    "start": "ts-node dist/main/Server.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main/Server.ts",
    "lint": "eslint 'src/**' --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "test:staged": "npm test -- --findRelatedTests",
    "test:cov": "npm test -- --coverage",
    "test:ci": "npm test -- --coverage -c jest-unit-config.js",
    "test:coveralls": "yarn test:cov && coveralls < coverage/lcov.info",
    "pretest:integration": "NODE_ENV=test yarn migration:run",
    "test:integration": "NODE_ENV=test yarn test -- --watch -c jest-integration-config.js"
  },
  "devDependencies": {
    "@sendgrid/mail": "^7.4.0",
